{
"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "Cornify!!",
  "version": "10002",
  "manifest_version": 2,
  "icons": {
    "48": "cornify_48.png",
    "128": "cornify_128.png"
  },

  "browser_action": {
    "default_icon": "cornify.ico"
  },

 "background": {
    "scripts": ["background.js"],
    "persistent": true
  },

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["midi.js", "cornify.js"],
      "css": ["button_chooser.css"],
      "run_at": "document_idle"
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],

  "web_accessible_resources": [
    "cornify.js", 
    "midi.js",

    "button_chooser.js",
    "button_chooser.css",
    "lib/jquery.js",
    "lib/jQuery.DomOutline/jquery.dom-outline-1.0.js",
    "lib/backbone/underscore.js",
    "lib/backbone/mustache.js",

    "sparkle.mp3"
  ]
}
